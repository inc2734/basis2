  "exports": {
    ".": "./dist/index.js",
    "./app": "./dist/app.js",
    "./library": "./dist/library.js"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/inc2734/unitone-css"
  },
  "files": [
