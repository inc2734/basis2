{
  "name": "@inc2734/unitone-css",
  "version": "0.42.3",
  "description": "",
  "main": "index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/inc2734/unitone-css"
  },
  "files": [
    "src",
    "dist",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "css"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "prepublish": "run-s build",
    "clean": "rimraf dist",
    "css:app": "sass --no-source-map src/app.scss:dist/app.css && postcss dist/app.css -r",
    "css:storybook": "sass --no-source-map src/storybook.scss:dist/storybook.css && postcss dist/storybook.css -r",
    "css": "run-p css:*",
    "js:app": "webpack ./src/app.js -o ./dist/ --output-filename app.js --config webpack.config.js",
    "js": "run-p js:*",
    "build": "run-s clean format css js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format:js": "prettier --write './src/**/*.{js,jsx}'",
    "format": "run-p format:*",
    "server": "storybook dev -p 6006",
    "public": "npm run build && storybook build -c ./.storybook -o ./public"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@storybook/addon-actions": "^7.0.5",
    "@storybook/addon-essentials": "^7.0.5",
    "@storybook/addon-links": "^7.0.5",
    "@storybook/react-webpack5": "^7.0.5",
    "@whitespace/storybook-addon-html": "^5.1.4",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.2",
    "cssnano": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "prettier": "^2.8.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-syntax-highlighter": "^15.5.0",
    "rimraf": "^5.0.0",
    "sass": "^1.62.0",
    "storybook": "next",
    "webpack": "^5.79.0",
    "webpack-cli": "^5.0.1"
  }
}
