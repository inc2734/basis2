{
  "name": "@inc2734/unitone-css",
  "version": "0.60.8",
  "description": "",
  "engines": {
    "node": ">=20.10.0",
    "npm": ">=10.2.3"
  },
  "volta": {
    "node": "20.11.1"
  },
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./app": "./dist/app.js",
    "./library": "./dist/library.js"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/inc2734/unitone-css"
  },
  "files": [
    "src",
    "dist",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "css"
  ],
  "scripts": {
    "prepublish": "run-s build",
    "clean": "rimraf dist",
    "css:app": "sass --no-source-map src/app.scss:dist/app.css && postcss dist/app.css -r",
    "css": "run-p css:*",
    "js": "rollup -c --bundleConfigAsCjs",
    "build": "run-s clean format css js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format:js": "prettier --write './src/**/*.{js,jsx}'",
    "format": "run-p format:*"
  },
  "devDependencies": {
    "@babel/core": "^7.24.9",
    "@babel/plugin-transform-runtime": "^7.24.7",
    "@babel/preset-env": "^7.24.8",
    "@babel/preset-react": "^7.24.7",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "autoprefixer": "^10.4.19",
    "babel-loader": "^9.1.3",
    "cssnano": "^7.0.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.39",
    "postcss-cli": "^11.0.0",
    "postcss-import": "^16.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-syntax-highlighter": "^15.5.0",
    "remark-gfm": "^4.0.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.18.1",
    "sass": "^1.77.8"
  },
  "workspaces": [
    "website"
  ]
}
